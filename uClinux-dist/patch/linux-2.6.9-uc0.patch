+	.name		= SERIAL_T32_NAME,
+	.write		= t32_console_write,
+	.device		= uart_console_device,
+	.setup		= t32_console_setup,
+	.flags		= CON_PRINTBUFFER,
+	.index		= -1,
+	.data		= &t32_reg,
+};
