+	if (retval)
+		return retval;
+#else /* emulation */
+	/* Initialize the work, and shcedule it. */
+	INIT_WORK(&t32_poll_task, t32_poll, port);
+	schedule_delayed_work(&t32_poll_task, 1);
+#endif
+
+	return 0;
+}
