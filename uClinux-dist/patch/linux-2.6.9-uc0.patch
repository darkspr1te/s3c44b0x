+
+static void puts_hex(unsigned long i)
+{
+	char lhex_buf[]="0x00000000";
+	unsigned long ii,v;
+
+	for(ii=9;ii>1;ii--)
+	{
+		v=(((0x0000000F << ((9-ii)*4)) & i) >> ((9-ii)*4));
+		if(v>9)
+			lhex_buf[ii]=(char)('A'+v-10);
+		else
+			lhex_buf[ii]=(char)('0'+v);
+	}
+
+	t32_puts(lhex_buf);
