+	.minor			= SERIAL_T32_MINOR,
+	.nr				= UART_NR,
+	.cons			= T32_CONSOLE,
+};
+
+static int __init
+t32_init(void)
+{
+	int ret;
+
+	printk(KERN_INFO "T32: JTAG1 Serial emulation driver driver $Revision: 1.1 $\n");
+
+	ret = uart_register_driver(&t32_reg);
+	if (ret == 0) {
+		int i;
+
+		for (i = 0; i < UART_NR; i++)
+			uart_add_one_port(&t32_reg, &t32_ports[i]);
+	}
+	return ret;
+}
