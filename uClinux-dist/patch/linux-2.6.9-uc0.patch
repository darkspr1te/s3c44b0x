+	if (info->xmit_tail info->xmit_cnt < SERIAL_XMIT_SIZE) {
+		xmit_string(info, info->xmit_buf info->xmit_tail,
+					info->xmit_cnt);
+		info->xmit_tail =
+			(info->xmit_tail info->xmit_cnt) & (SERIAL_XMIT_SIZE - 1);
+		info->xmit_cnt = 0;
+	} else {
