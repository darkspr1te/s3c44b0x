+	.write          = s3c44b0x_console_write,
+	.setup          = s3c44b0x_console_setup,
+	.flags          = CON_PRINTBUFFER,
+	.index          = -1,
+};      
+
+static int __init s3c44b0x_console_init(void) 
+{                               
+	register_console(&s3c44b0x_con_driver);
