+        #if 1   
+                        /* r0     r6      r5   lr */
+                .macro  get_irqnr_and_base, irqnr, irqstat, base, tmp
+                ldr    \irqstat, =(rINTBase + 0x3C) @ rIRQISPR
+                ldr    \irqstat,   [\irqstat, #0]   @ load irqstat
+
+                mov    \irqnr, #0 
+1001:           
+                tst       \irqstat, #1
+                bne     1002f
+                add     \irqnr, \irqnr, #1
+                mov    \irqstat, \irqstat, lsr #1
+                cmp    \irqnr, #NR_IRQS 
+                bcc     1001b
+1002:           /* EQ will be set if we reach 32 */
