+        if (ser->type != PORT_UNKNOWN && ser->type != PORT_T32_JTAG1)
+                ret = -EINVAL;
+        if (ser->irq < 0 || ser->irq >= NR_IRQS)
+                ret = -EINVAL;
+        if (ser->baud_base < 9600)
