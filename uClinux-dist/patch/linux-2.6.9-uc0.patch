+
+static unsigned char eb01_irq_prtable[32] = {
+        7 << 5, /* FIQ */
+        0 << 5, /* SWIRQ */
+        0 << 5, /* US0IRQ */
+        0 << 5, /* US1IRQ */
+        2 << 5, /* TC0IRQ */
+        2 << 5, /* TC1IRQ */
+        2 << 5, /* TC2IRQ */
+        0 << 5, /* WDIRQ */
