+static void __s3c4510b_stop_rx(struct uart_port *port)
+{
+	struct uart_regs *uart = (struct uart_regs *)port->iobase;
+	UART_CTRL      uctrl;
+
+	_DPRINTK("called with port = 0x%08x", (unsigned int) port);
+
+	uctrl.ui = uart->m_ctrl.ui;
+	uctrl.bf.rxMode = 0x0;
+	uart->m_ctrl.ui = uctrl.ui;
