+	int verify = 1;
+	printk("Load initrd image from flash(%08x) to SDRAM(%08x), Length = %d ...", src, dst, count);
+	memmove(dst, src, count);
+	if (verify) {
+		printk("Verify ...");
+		printk("%s\n", memcmp(src, dst, count) == 0 ? "Done" : "Failed");
+	} else {
+		printk("Done\n");
+	}
+}
