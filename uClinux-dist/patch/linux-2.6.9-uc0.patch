+	*(volatile unsigned short *)0xfffff428 |= 0x0101; /* not sleeping */
+	*(volatile unsigned  char *)0xfffff42b &= ~0x02; /* input irq5 */
+	*(volatile unsigned short *)0xfffff428 &= ~0x0202; /* irq5 fcn on */
