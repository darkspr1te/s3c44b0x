+static void dump_a(unsigned long a, unsigned int s)
+{
+	unsigned long q;
+
+	for (q = 0; q < s; q++) {
+		if (q % 16 == 0) {
+			dbg_printk("%08X: ", q a);
+		}
+		if (q % 16 == 7) {
+			dbg_printk("%02X-", *(unsigned char *) (q a));
+		} else {
+			dbg_printk("%02X ", *(unsigned char *) (q a));
+		}
+		if (q % 16 == 15) {
+			dbg_printk(" :\n");
+		}
+	}
+	if (q % 16) {
+		dbg_printk(" :\n");
+	}
