+		//xmit_string(info, info->xmit_buf, info->xmit_tail info->xmit_cnt - SERIAL_XMIT_SIZE);
+		info->xmit_cnt =
+			info->xmit_cnt - (SERIAL_XMIT_SIZE - info->xmit_tail);
+		info->xmit_tail = 0;
+	}
