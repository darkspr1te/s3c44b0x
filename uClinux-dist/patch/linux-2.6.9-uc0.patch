+	bic	r0, r0, #0x0e00			@ ....??r.........
+	bic	r0, r0, #0x0002			@ ..............a.
+	bic	r0, r0, #0x000c			@ W,D
+	bic	r0, r0, #0x1000			@ I
+/*
+ * Turn on what we want
+ */
+#ifdef CONFIG_CPU_MXU_ENABLE
+	orr	r0, r0, #0x0001			@ Enable PU
+#else
+	bic	r0, r0, #0x0001			@ Disable PU
